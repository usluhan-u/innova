{
  "name": "innova",
  "description": "Innova",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon",
    "dev:client": "next dev",
    "copyfiles:media": "copyfiles -u 1 \"media/**/*.{svg,jpg,png}\" dist/media",
    "copyfiles:lottie": "copyfiles -u 1 \"lottie/**/*.{svg,jpg,png}\" dist/lottie",
    "build:client": "next build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build": "cross-env NODE_ENV=production rimraf .next build dist && yarn build:payload && yarn build:server && yarn copyfiles:media && yarn copyfiles:lottie && yarn build:client",
    "serve": "cross-env NODE_ENV=production PAYLOAD_CONFIG_PATH=dist/src/payload.config.js node dist/src/server.js",
    "syncsearch:dev": "ts-node src/sync-search.ts",
    "lint": "eslint src/**/*.{ts,tsx} --fix",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "husky": "husky install"
  },
  "dependencies": {
    "@ajna/pagination": "^1.4.19",
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/next-js": "^2.2.0",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@faceless-ui/slider": "^1.1.17",
    "@fontsource/inter": "^5.0.16",
    "@lottiefiles/react-lottie-player": "^3.5.3",
    "@meilisearch/instant-meilisearch": "^0.14.0",
    "@payloadcms/plugin-form-builder": "^1.1.0",
    "@payloadcms/plugin-nested-docs": "^1.0.10",
    "@payloadcms/plugin-redirects": "^1.0.1",
    "@payloadcms/plugin-seo": "^1.0.15",
    "@react-hook/hover": "^4.0.0",
    "algoliasearch": "^4.22.0",
    "date-fns": "^3.0.6",
    "dotenv": "^16.3.1",
    "escape-html": "^1.0.3",
    "express": "^4.18.2",
    "framer-motion": "^10.16.16",
    "instantsearch.js": "^4.63.0",
    "is-url-http": "^2.3.7",
    "meilisearch": "^0.36.0",
    "next": "^13.5.6",
    "nodemailer": "^6.9.7",
    "payload": "^1.15.8",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.49.2",
    "react-icons": "^4.12.0",
    "react-instantsearch-dom": "^6.40.4",
    "react-instantsearch-hooks-web": "^6.47.3",
    "react-no-ssr": "^1.1.0",
    "slate": "^0.101.5",
    "slate-react": "^0.101.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/escape-html": "^1.0.4",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/qs": "^6.9.11",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/react-helmet": "^6.1.11",
    "@types/react-instantsearch-dom": "^6.12.7",
    "@types/react-no-ssr": "^1.1.7",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "packageManager": "yarn@4.0.2"
}
